#include <main.h>

void main()
{

   output_b(0xFF);
   set_tris_b(0xFF);
   set_tris_d(0x00);
   output_d(0x00);

   lcd_init();   
   lcd_gotoxy(1,1);   lcd_putc("BALL AND BEAM");
   lcd_gotoxy(1,2);   lcd_putc("GELEN 321");
   while(TRUE)
   {

      //Example blinking LED program
      output_low(LED);
      delay_ms(DELAY);
      output_high(LED);
      delay_ms(DELAY);

      //TODO: User Code
      
      
      printf("deneme\r\n");
    
   }

}
